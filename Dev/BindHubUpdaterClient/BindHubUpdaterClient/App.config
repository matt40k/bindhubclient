<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
    </startup>
  <appSettings>
    <add key="API_Url" Value="https://www.bindhub.com/api/" />
    <add key="API_User" Value="username" />
    <add key="API_Key" Value="a_very_random_code" />
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" autoReload="true">
    <targets>
      <target name="system" xsi:type="File" fileName="${specialfolder:folder=CommonApplicationData}/BindHub/Logs/log.txt" archiveFileName="${specialfolder:folder=CommonApplicationData}/SIMSBulkImport/Logs/Archive/log_{#}.txt" archiveEvery="Day" archiveNumbering="Rolling" maxArchiveFiles="7" layout="${longdate}|${level}|${message} ${exception:format=tostring}" />
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="system" />
    </rules>
  </nlog>
</configuration>
